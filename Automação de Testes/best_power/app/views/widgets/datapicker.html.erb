    </div>

    <div class="row">
      <div class="col s3"></div>
        <div class="col s8 tamanhodiv2 center">
          <h5 class="">Data Picker!!</h5>
        </div>
        <div class="col s2"></div>
      </div>

      <div class="row">
       <div class="col s3"></div>
        <div class="col s8">
         <p class="">Date: <input type="text" id="datepicker"></p>
        </div>
        <div class="col s2"></div>
      </div>

      <div class="row">
        <div class="center">
          <%= link_to "Voltar", treinamento_home_path, :method => "get", class:  "btn red"%>
        </div>
      </div>

    </div>

  </div>

</div>

<script>
$( function() {
